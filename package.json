{
  "name": "typeguard-composer",
  "version": "0.9.9",
  "description": "Composable type guards for runtime data validation.",
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "echo \"Error: tslint not added yet.\" && exit 1",
    "test": "jest --coverage --silent",
    "test:nocoverage": "jest",
    "publish": "",
    "postpublish": "git push && git push --tags",
    "prepublishOnly": "npm test && npm run build && npm version patch -m 'chore: bump version to %s'",
    "precommit": "lint-staged",
    "coverage": "codecov -f coverage/*.json"
  },
  "lint-staged": {
    "*.{ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoNameProvided/typeguard-composer.git"
  },
  "keywords": [
    "composable",
    "type",
    "guards",
    "typescript"
  ],
  "author": "Attila Olah <NoNameProvided@users.noreply.github.com >",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoNameProvided/typeguard-composer/issues"
  },
  "homepage": "https://github.com/NoNameProvided/typeguard-composer#readme",
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.0",
    "codecov": "3.6.5",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  }
}
